{
  "name": "astro-live-compiler",
  "version": "0.0.1",
  "description": "Live preview of the TS output of Astro components",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "tsc:lint": "tsc",
    "build": "pnpm tsc:lint && vite build && pnpm postbuild",
    "build:ci": "vite build && pnpm postbuild",
    "postbuild": "esno ./scripts/postbuild.ts",
    "tsc": "tsc",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@astrojs/compiler": "^2.10.3",
    "@types/node": "^22.10.5",
    "autoprefixer": "^10.4.20",
    "glob": "^11.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-solid": "^2.11.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "dependencies": {
    "@fontsource/fira-code": "^5.0.16",
    "@fontsource/montserrat": "^5.0.16",
    "@isaacs/ttlcache": "^1.4.1",
    "@kobalte/core": "^0.13.7",
    "@solid-primitives/media": "^2.2.6",
    "@solid-primitives/scheduled": "^1.4.2",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "emmet-monaco-es": "^5.3.2",
    "esno": "^4.8.0",
    "fflate": "^0.8.2",
    "monaco-editor": "^0.44.0",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "monaco-volar": "^0.4.0",
    "onigasm": "^2.2.5",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.3",
    "solid-sonner": "^0.2.8",
    "split.js": "^1.6.5",
    "tailwindcss": "^3.4.17",
    "theme-vitesse": "^0.8.3"
  }
}
