{
  "name": "astro-live-compiler",
  "version": "0.0.1",
  "description": "Live preview of the TS output of Astro components",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && pnpm postbuild",
    "postbuild": "node ./scripts/postbuild.cjs",
    "build:ci": "pnpm i @astrojs/compiler@latest && pnpm build",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.1",
    "autoprefixer": "^10.4.16",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "solid-devtools": "^0.29.2",
    "typescript": "^5.3.2",
    "vite": "^4.0.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-solid": "^2.7.2",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@astrojs/compiler": "^2.3.2",
    "@fontsource/fira-code": "^5.0.15",
    "@fontsource/montserrat": "^5.0.15",
    "@kobalte/core": "^0.11.2",
    "@solid-primitives/media": "^2.2.5",
    "@solid-primitives/scheduled": "^1.4.1",
    "emmet-monaco-es": "^5.3.0",
    "fflate": "^0.8.1",
    "monaco-editor": "^0.44.0",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "monaco-volar": "^0.4.0",
    "onigasm": "^2.2.5",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.6",
    "split.js": "^1.6.5",
    "tailwindcss": "^3.3.5",
    "theme-vitesse": "^0.7.5"
  }
}
