{
  "name": "astro-live-compiler",
  "version": "0.0.1",
  "description": "Live preview of the TS output of Astro components",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && pnpm postbuild",
    "postbuild": "node ./scripts/postbuild.cjs",
    "build:ci": "pnpm i @astrojs/compiler@latest && pnpm build",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.14.6",
    "autoprefixer": "^10.4.13",
    "chokidar": "^3.5.3",
    "glob": "^10.0.0",
    "kleur": "^4.1.5",
    "ora": "^6.2.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "solid-devtools": "^0.26.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-solid": "^2.5.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "dependencies": {
    "@astrojs/compiler": "1.3.0",
    "@fontsource/fira-code": "^4.5.13",
    "@fontsource/montserrat": "^4.5.14",
    "@kobalte/core": "^0.6.1",
    "@solid-primitives/media": "^2.1.6",
    "@solid-primitives/scheduled": "^1.3.0",
    "emmet-monaco-es": "^5.2.1",
    "fflate": "^0.7.4",
    "monaco-editor": "^0.36.1",
    "monaco-editor-textmate": "^4.0.0",
    "monaco-textmate": "^3.0.1",
    "onigasm": "^2.2.5",
    "solid-icons": "^1.0.4",
    "solid-js": "^1.6.10",
    "split.js": "^1.6.5",
    "tailwindcss": "^3.2.7",
    "theme-vitesse": "^0.6.3"
  }
}
